{
  "name": "videogame-gallery",
  "version": "1.0.0",
  "description": "Front end browser for your videogame roms collection",
  "main": "main.js",
  "scripts": {
    "build": "browserify -t [ babelify --presets [ react ] ] assets/js/web-app.js -o assets/js/web-app.min.js -d",
    "watch": "watchify -t [ babelify --presets [ react ] ] assets/js/web-app.js -o assets/js/web-app.min.js -v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kirilldev/videogame-gallery.git"
  },
  "keywords": [
    "emulation"
  ],
  "author": "Kirill Reznikov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kirilldev/videogame-gallery/issues"
  },
  "homepage": "https://github.com/kirilldev/videogame-gallery#readme",
  "dependencies": {
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "express": "^4.13.4",
    "fs2json": "^0.1.4",
    "jquery": "^2.2.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "three": "^0.73.2"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "browserify": "latest",
    "watchify": "latest"
  }
}
